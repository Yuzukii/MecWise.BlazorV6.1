@page "/SignedOut"
@model Microsoft.Identity.Web.UI.Areas.MicrosoftIdentity.Pages.Account.SignedOutModel

@namespace MecWise.Blazor.Host.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />

    <title>MecWise</title>
    <base href="~/" id="baseUrl" />

    <script src="lib/jquery/jquery.min.js"></script>

    <script src="lib/bootstrap/js/bootstrap.min.js"></script>
    <link href="lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <script src="lib/bootstrap/js/bootstrap-dialog.min.js"></script>
    <link href="lib/bootstrap/css/bootstrap-dialog.min.css" rel="stylesheet" />

    <link href="lib/open-iconic/css/open-iconic-bootstrap.min.css" rel="stylesheet" />
    <link href="lib/fontawesome/css/all.min.css" rel="stylesheet" />

    <script src="js/MecWise.Blazor.ScriptLoader.js"></script>

</head>
<body>
    <div class="login blue-gradient color-block z-depth-1">
        <div class="login-container" style="font-size:1.2rem;min-height:300px">
            <div class="container p-4 text-center">
                <div class="login-logo-container"></div>
                <p style="margin-top:10px;color:red">You have logout successfully.</p>
                <a href="javascript:window.location.replace('');">Click here to login again.</a>
            </div>
        </div>
    </div>
</body>
</html>



